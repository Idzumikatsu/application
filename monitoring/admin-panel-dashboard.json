{
  "dashboard": {
    "id": null,
    "title": "Admin Panel Dashboard",
    "tags": ["admin-panel", "crm"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Admin Panel Response Time",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(http_server_requests_seconds_bucket{job=\"admin-panel-backend\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"admin-panel-backend\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_server_requests_seconds_bucket{job=\"admin-panel-backend\"}[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Response Time"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Admin Panel Memory Usage",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"admin-panel-backend\", area=\"heap\"}",
            "legendFormat": "Used Heap"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"admin-panel-backend\", area=\"heap\"}",
            "legendFormat": "Max Heap"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Admin Panel CPU Usage",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"admin-panel-backend\"}[5m])",
            "legendFormat": "CPU Usage"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "CPU Usage"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Database Connections",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "Active Connections"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Connections"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Total Admin Requests",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(http_server_requests_seconds_count{job=\"admin-panel-backend\"})",
            "legendFormat": "Total Requests"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["last"]
          }
        }
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Error Rate",
        "gridPos": {
          "x": 6,
          "y": 16,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"admin-panel-backend\", code=~\"5..\"}[5m]) / rate(http_server_requests_seconds_count{job=\"admin-panel-backend\"}[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["last"]
          }
        }
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Frontend Page Load Time",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(frontend_page_load_time_seconds_bucket{job=\"admin-panel-frontend\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(frontend_page_load_time_seconds_bucket{job=\"admin-panel-frontend\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Page Load Time"
          },
          {
            "format": "short"
          }
        ]
      }
    ]
  },
  "overwrite": true
}